;\n  //   courageousIconRef.current.addEventListener(\"mouseout\", () =>\n  //     removeHover(courageousIconRef)\n  //   );\n  // }, []);\n\n  return (\n    <header>\n      <p onClick={() => (window.location = \"/\")}>Courageous Showcase</p>\n      <HamburgerMenu visible={burgerVisible} setVisible={setBurgerVisible} />\n      <div className=\"icons\">\n        <a\n          href=\"https://www.courageousstudios.com\"\n          className=\"desktop\"\n          id=\"courageous-container\"\n          target=\"_blank\"\n          rel=\"noreferrer\"\n        >\n          <Courageous\n            fill=\"#ffffff\"\n            stroke=\"#ffffff\"\n            id=\"courageous\"\n            // className=\"\"\n            ref={courageousIconRef}\n          />\n        </a>\n        <a\n          href=\"https://vimeo.com/user62308493\"\n          className=\"desktop\"\n          id=\"vimeo-container\"\n          target=\"_blank\"\n          rel=\"noreferrer\"\n        >\n          <Vimeo\n            fill=\"#ffffff\"\n            stroke=\"#ffffff\"\n            id=\"vimeo\"\n            ref={vimeoIconRef}\n          />\n        </a>\n        <a\n          href=\"https://www.linkedin.com/company/courageous-studios\"\n          target=\"_blank\"\n          className=\"desktop\"\n          id=\"linkedin-container\"\n          target=\"_blank\"\n          rel=\"noreferrer\"\n        >\n          <Linkedin\n            fill=\"#ffffff\"\n            stroke=\"#ffffff\"\n            id=\"linkedin\"\n            ref={linkedinIconRef}\n          />\n        </a>\n        <a\n          href=\"https://www.instagram.com/courageous_studios\"\n          className=\"desktop\"\n          id=\"insta-container\"\n          target=\"_blank\"\n          rel=\"noreferrer\"\n        >\n          <Insta\n            fill=\"#ffffff\"\n            stroke=\"#ffffff\"\n            id=\"insta\"\n            ref={instaIconRef}\n          />\n        </a>\n        <img\n          className=\"mobile\"\n    